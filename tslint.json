{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-multiline-js": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
